
{
  "expo": {
    "name": "CryptoTrail",
    "slug": "cryptotrail",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true",
      "bundleIdentifier": "com.caseyjordan.cryptotrail"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.caseyjordan.cryptotrail"
    },
    "web": {
      "favicon": "./assets/icon.png"
    }
  }
}
